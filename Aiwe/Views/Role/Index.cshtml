@model List<RoleViewModel>
@{
  ViewBag.Title = Aibe.LCZ.W_Roles;
  ViewBag.PageTitle = Aibe.LCZ.W_Roles;
  ViewBag.ActionName = Aibe.DH.IndexActionName;
  ViewBag.ControllerName = Aiwe.DH.MvcRoleControllerName;
  ViewBag.FilterName = "Role"; //used for correct filter rendering in the common
